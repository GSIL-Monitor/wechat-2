<template name="banner">
    <view class="navlist clearfix">
        <template is="bannerIcon" data="{{item}}" wx:for="{{bannerIcon}}" wx:key="{{item.Id}}" />
    </view>
</template>

<template name="bannerIcon">
    <navigator url="{{item.Link}}">
        <image src="{{item.Pic}}" alt=""></image>
        <text>{{item.Name}}</text>
    </navigator>
</template>